type Family {
    id: ID! @id
    family_name: String!
}

type People {
    id: ID! @id
    person_name: String!
    family: Family!
}

type Survey {
    id: ID! @id
    dateTaken: DateTime! @createdAt 
    survey_name: String!
    employee: Employee!
  
}

type Employee {
    id: ID! @id
    name: String!
    type: String!
}

type Question {
    id: ID! @id
    survey: Survey!
    family: Family!
    question_text: String!
}

type Answers {
    id: ID! @id
    dateTaken: DateTime! @createdAt
    answer: String!
    question: Question!
    family: Family!
    respondent: People!
} 